@model PSClubs.Models.GroupMember
@using Microsoft.AspNetCore.Http
@{
    ViewData["Title"] = $"delete member from {Context.Session.GetString("name")}";
}

<h1>@ViewData["Title"]</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>GroupMember</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateJoined)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateJoined)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateLeft)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateLeft)
        </dd>
        <dt class="col-sm-2">
